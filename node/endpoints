  Class Imaged
    String name
    Url<Image> thumbnail = "maps/$name/thumbnail"

  Class Server
    String name
    String host
    Integer port
    Mastermode mastermode
    mutable Maybe<Player> master

  Class Map
    String name
    Maybe<Url<Image>> thumbnail = "maps/$name/thumbnail"

  Class Playermodel
    String name
    Mayve<Url<Image>> thumbnail = "playermodels/$name/thumbnail"

  Class Crosshair
    String name
    Maybe<Url<Image>> thumbnail = crosshairs/$name/thumbnail"
    Maybe<Integer> size # not set for the crosshair list

  Class Player
    mutable String name
    mutable String model
    mutable maybe<String> team # Set if mode with teams
    mutable Boolean spectating
    Maybe bot/ # Set if this is a bot
      Player controller
      Integer skill

    Function kick

